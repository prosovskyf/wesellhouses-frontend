{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "source": {
    "include": [
      "./src/components",
      "./src/context",
      "./src/App.js",
      "./src/index.js"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|docs)"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/better-docs/component",
    "node_modules/better-docs/category"
  ],
  "templates": {
    "better-docs": {
      "name": "We Sell Houses Components"
    }
  },
  "opts": {
    "destination": "docs",
    "encoding": "utf8",
    "recurse": true,
    "verbose": true,
    "template": "node_modules/better-docs/"
  }
}